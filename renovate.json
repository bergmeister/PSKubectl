{
  "extends": ["config:base", ":maintainLockFilesMonthly"],
  "semanticCommits": true,
  "prCreation": "not-pending",
  "rangeStrategy": "replace",
  "nuget": {
    "enabled": true,
    "rollbackPrs": false
  },
  "ignoreDeps": ["Microsoft.PowerShell.SDK"],
  "packageRules": [
    {
      "packagePatterns": ["^KubeClient"],
      "groupName": "kubeclient"
    }
  ]
}
